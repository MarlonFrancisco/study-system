{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["./src/**/*.json"]
    }
}
